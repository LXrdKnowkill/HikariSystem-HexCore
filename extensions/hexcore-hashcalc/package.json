{
	"name": "hexcore-hashcalc",
	"displayName": "HexCore Hash Calculator",
	"description": "Calculate MD5, SHA1, SHA256, SHA512 hashes for file integrity verification and malware analysis",
	"version": "1.1.0",
	"publisher": "hikarisystem",
	"license": "MIT",
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"hash",
		"md5",
		"sha1",
		"sha256",
		"sha512",
		"checksum",
		"malware"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "hexcore.hashcalc.calculate",
				"title": "HexCore: Calculate File Hashes"
			},
			{
				"command": "hexcore.hashcalc.quick",
				"title": "HexCore: Quick Hash (SHA-256)"
			},
			{
				"command": "hexcore.hashcalc.verify",
				"title": "HexCore: Verify Hash"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "hexcore.hashcalc.calculate",
					"group": "hexcore@2"
				}
			]
		}
	},
	"scripts": {
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^18.0.0",
		"@types/vscode": "^1.60.0",
		"typescript": "^5.0.0"
	}
}
